{
  "compilerOptions": {
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2015",
    "useUnknownInCatchVariables": false,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    
    "composite": true,
    "resolveJsonModule": true,
  },
  // js files should be ts...?
  "include": ["src", "*.tsx", "*.ts",  "App.tsx", "index.js", "app.json", "shim.js", "init.js"],
  "exclude": [
    "babel.config.js", // Exclude babel.config.js
    "metro.config.js",
    "jest.config.js",
    "aztec-scripts",
    "node_modules",    // Exclude node_modules
    "ios",             // Exclude ios folder if not needed
    "android"          // Exclude android folder if not needed
  ],
