{
  "name": "dry-aztec",
  "version": "2.1.0",
  "private": false,
  "scripts": {
    "start": "react-native esbuild-start --reset-cache",
    "build": "react-native esbuild-bundle --bundle-output build/bundle.js --platform ios --reset-cache",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "run:ios": "react-native run-ios --no-packager",
    "run:android": "react-native run-android",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "lint:fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "flow": "./node_modules/flow-bin/cli.js",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@aztec/accounts": "0.41.0",
    "@aztec/aztec.js": "0.41.0",
    "@aztec/noir-contracts.js": "0.41.0",
    "@babel/preset-typescript": "^7.24.7",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@tamagui/config": "^1.79.17",
    "@tamagui/lucide-icons": "^1.102.1",
    "date-fns": "^3.6.0",
    "esbuild": "^0.17.19",
    "ethers": "^6.13.2",
    "global": "^4.4.0",
    "i18n-js": "^3.2.1",
    "jetifier": "^1.6.5",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-asset": "^2.1.1",
    "react-native-cli": "^2.0.1",
    "react-native-esbuild": "^0.6.0",
    "react-native-polyfill-globals": "^3.1.0",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.6.0",
    "tamagui": "^1.79.17",
    "victory-native": "^31.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.6.3",
    "danger": "^7.0.2",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.105.0",
    "jest": "^29.6.3",
    "jsdom": "13.0.0",
    "jsdom-global": "3.0.2",
    "plop": "^2.2.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "sinon": "^7.1.1",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!android/**",
      "!ios/**",
      "!assets/**"
    ],
    "coverageDirectory": "jest_coverage",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone XS"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/ReactNativeStarter.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/ReactNativeStarter.xcodeproj -scheme ReactNativeStarter -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest"
  }
}